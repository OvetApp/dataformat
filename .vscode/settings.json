{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.retool_types/**": true,
        "**/*tsconfig.json": true,
        ".cache": true,
        "retool.config.json": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "records/immunization*.json",
                "records/goal*.json",
                "records/encounter*.json",
                "records/observation*.json",
                "records/patient*.json",
                "records/person*.json",
                "records/relatedPerson*.json"
            ],
            "url": "./fhirR4.schema.json"
        },
    ]
}