{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.retool_types/**": true,
        "**/*tsconfig.json": true,
        ".cache": true,
        "retool.config.json": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                  "*immunization*.json",
                    "*goal*.json",
                    "*encounter*.json",
                    "*observation*.json",
                    "*patient*.json",
                    "*person*.json",
                    "*relatedPerson*.json",
                    "*condition*.json"
            ],
            "url": "./fhirR4.schema.json"
        },
    ]
}